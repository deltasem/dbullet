create table "@Model.Name" (
@for (int i = 0; i < Model.Columns.Count; i++){
@dbullet.core.engine.Oracle.OracleTemplateManager.BuildColumnCreateCommand(Model.Columns[i])
if (Model.Columns[i].Constraint is dbullet.core.dbo.Default) {
@: constraint @Model.Columns[i].Constraint.Name default '@(dbullet.core.engine.MsSql.MsSqlTemplateManager.GetDefaultValue((dbullet.core.dbo.Default)Model.Columns[i].Constraint))'
}
@(i != Model.Columns.Count - 1 ? ", " : "")
}
@{
	dbullet.core.dbo.Column pkColumn = Model.Columns.FirstOrDefault(p => p.Constraint != null);
	dbullet.core.dbo.PrimaryKey pk = null;
	if (pkColumn != null)
	{
		pk = pkColumn.Constraint as dbullet.core.dbo.PrimaryKey;
	}
}
@if (pk != null){
@:, constraint @pk.Name primary key clustered([@pkColumn.Name] asc) with (PAD_INDEX  = OFF, STATISTICS_NORECOMPUTE  = OFF, IGNORE_DUP_KEY = OFF, ALLOW_ROW_LOCKS  = ON, ALLOW_PAGE_LOCKS  = ON) ON [@pk.PartitionName]
}
) tablespace "@Model.PartitionName";
@*Идентити в оракле будут через sequence*@
@if(Model.Columns.Any(x => x.Identity))
{
	dbullet.core.dbo.Column identity = Model.Columns.FirstOrDefault(x => x.Identity);
 @:create sequence @(Model.Name+"_"+@identity.Name)
 @:minvalue 1
 @:start with 1
 @:increment by 1;
}