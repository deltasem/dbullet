@{System.Reflection.PropertyInfo[] props = (System.Reflection.PropertyInfo[])Model[1]; string[] values = (string[])Model[2];}
delete from "@Model[0].ToString().ToUpper()"
@if (props != null)
{
 @:where
	for (int i = 0; i < props.Length; i++)
	{
 @:@(i == 0 ? "" : "and ")"@props[i].Name.ToUpper()" = '@values[i]'
	}
}
