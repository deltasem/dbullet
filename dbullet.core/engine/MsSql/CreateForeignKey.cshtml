@{dbullet.core.dbo.ForeignKey fk = Model;}
@helper ForeignActionName(dbullet.core.dbo.ForeignAction action)
{
	switch(action)
	{
		case dbullet.core.dbo.ForeignAction.Cascade:
@:cascade
			break;
		case dbullet.core.dbo.ForeignAction.NoAction:
@:no action
			break;
		case dbullet.core.dbo.ForeignAction.SetNull:
@:set null
			break;
	}
}
alter table [@fk.SrcTableName] 
add constraint 
@fk.Name foreign key ([@fk.SrcColumnName]) 
references 
[@fk.RefTableName] ([@fk.RefColumnName]) 
on update no action 
on delete @ForeignActionName(fk.DeleteAction)